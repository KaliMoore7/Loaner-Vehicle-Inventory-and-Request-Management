        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    var vs = current.vehicle_status;
    // Add your code here
    var gr = new GlideRecord('x_919571_lva_loaner_vehicle_catalog');
    gr.addQuery('sys_id', current.vehicle);
    gr.query();

    if (gr.next()) {
        if (vs == 'Pending Release' || vs == 'Out on Field' || vs == 'Returned for Inspection' || vs == 'Sent for Servicing') {
            gr.status = 'Unavailable';
        } else if (vs == 'Returned to Warehouse') {
            gr.status = 'Available';
        } else if (vs == 'Decommissioned') {
            gr.status = 'Decommissioned';
        }
        gr.update();
    }
})(current, previous);]]></script>
